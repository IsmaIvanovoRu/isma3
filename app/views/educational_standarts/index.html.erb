<section class=panel>
  <h1>Образовательные стандарты</h1>
  <div>
    <%= link_to "Добавить", new_educational_standart_path, class: 'button' %>
  </div>
  <ul>
    <% @educational_standarts.each do |es| %>
      <div class="right">
        <% if @moderator_permission %>
          <%= link_to edit_educational_standart_path(es), title: 'Редактировать' do %>
            <i class="fi-page-edit"></i>
          <% end %>
          <%= link_to es, title: 'Удалить', method: :delete, data: { confirm: 'Are you sure?' } do %>
            <i class="fi-x"></i>
          <% end %>
        <% end %>
      </div>
        <li><%= link_to es.name, es %></li>
    <% end %>
  </ul>
</section>